<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CalculaListaActivity">

    <TextView
        android:id="@+id/btnCalculadoraLista"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sumar dos numero por lista"
        tools:layout_editor_absoluteX="137dp"
        tools:layout_editor_absoluteY="32dp" />

    <TextView
        android:id="@+id/txtNumerolist1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Ingrese Numero 1"
        tools:layout_editor_absoluteX="41dp"
        tools:layout_editor_absoluteY="109dp" />

    <EditText
        android:id="@+id/editTextNumberList1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="number"
        tools:layout_editor_absoluteX="158dp"
        tools:layout_editor_absoluteY="93dp" />

    <TextView
        android:id="@+id/txtNumeroList2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Ingrese Numero 2"
        tools:layout_editor_absoluteX="43dp"
        tools:layout_editor_absoluteY="191dp" />

    <EditText
        android:id="@+id/EdiNumeroList2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="number"
        tools:layout_editor_absoluteX="159dp"
        tools:layout_editor_absoluteY="171dp" />
</androidx.constraintlayout.widget.ConstraintLayout>